{
  "name": "npu-ego-github-io",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "DOCUSAURUS_BROWSER=none BROWSER=none NODE_OPTIONS=\"--require=./scripts/resolveWeakShim.cjs --experimental-loader=./scripts/esmCssLoader.mjs\" docusaurus start",
    "build": "DOCUSAURUS_SKIP_SSG=true DANGEROUSLY_DISABLE_SSG=true NODE_OPTIONS=\"--require=./scripts/resolveWeakShim.cjs --experimental-loader=./scripts/esmCssLoader.mjs\" docusaurus build",
    "build:ssg": "NODE_OPTIONS=\"--require=./scripts/resolveWeakShim.cjs --experimental-loader=./scripts/esmCssLoader.mjs\" docusaurus build",
    "test:ssg-integration": "node scripts/test_ssg_integration.cjs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@docusaurus/remark-plugin-npm2yarn": "^3.9.2",
    "@docusaurus/theme-classic": "^3.9.2",
    "@theme/prism-include-languages": "file:./packages/prism-include-languages",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@tsconfig/docusaurus": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0"
  }
